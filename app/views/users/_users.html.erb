<% if users.any? %>
  <ul class="list-unstyled ">
    <% users.each do |user| %>
      <li class="media pb-4">
        <div class="rounded-circle">
          <% if user.image? %>
            <%= image_tag user.image.thumb130.url %>
          <% else %>
            <%= image_tag "/assets/icon.jpg", :size => '130x130' %>
          <% end %>
        </div>
        <div class="media-body pl-3">
          <div>
            <%= user.name %>
          </div>
          <div>
            <p><%= link_to 'profile', user_path(user) %></p>
          </div>
        </div>
      </li>
    <% end %>
  </ul>
  <%= paginate users %>
<% end %>